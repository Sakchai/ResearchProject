@model ProjectModel

<div asp-validation-summary="ModelOnly"></div>


<div class="panel-group">
    <div class="panel panel-default">
        <div class="panel-body">
            <div class="panel-group">
                <div class="col-md-3">
                    <nop-label asp-for="ProjectStatusId" />
                </div>
                <div class="col-md-9">
                    <nop-select asp-for="ProjectStatusId" asp-items="Model.AvailableProjectStatuses" />
                    <span asp-validation-for="ProjectStatusId"></span>
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-3">
                    <nop-label asp-for="StartContractDate" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="StartContractDate" />
                    <span asp-validation-for="StartContractDate"></span>
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-3">
                    <nop-label asp-for="EndContractDate" />
                </div>
                <div class="col-md-9">
                    <nop-editor asp-for="EndContractDate" />
                    <span asp-validation-for="EndContractDate"></span>
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-3">
                    <nop-label asp-for="Comment" />
                </div>
                <div class="col-md-9">
                    <nop-textarea asp-for="Comment" />
                    <span asp-validation-for="Comment"></span>
                </div>
            </div>
        </div>
    </div>
</div>